{
  "author": "Abekenman",
  "background": {
    "scripts": ["background.js"],
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Abekenman",
    "default_icon": "Abekenman.svg",
    "default_popup": "popup.html"
  },
  '//' : '"browser_specific_settings": {
  },
  "chrome_settings_overrides": {
    "homepage": "homepage.html",
    "search_provider": {
      "name": "Abekenman",
      "search_url": "https://google.com/search?q={searchTerms}",
      "is_default": true,
      "alternate_urls": "https://www.google.com/search?p={searchTerms}",
      "encoding": "UTF-8",
      "favicon_url": "https://abekenman.github.io/Abekenman.svg",
      "image_url": "https://lens.google.com/search?p={searchTerms}",
      "image_url_post_params": "",
      "instant_url": "",
      "instant_url_post_params": "",
      "keyword": "",
      "prepopulated_id": "",
      "search_url_post_params": "",
      "suggest_url": "",
      "suggest_url_post_params": ""
    },
  },',
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "matches": ["*://example.com/*"],
      "js": ["cs.js"],
      "run_at": "document_start",
      "css": ["cs.css"]
    }
  ],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
  "default_locale": "ja",
  "description": "【Abekenman】のPWA",
  "homepage_url": "https://abekenman.github.io/",
  "manifest_version": 1,
  "name": "Abekenman",
  "options_ui": {
    "page": "option.html"
  },
  "page_action": {
    "default_title": "Abekenman",
    "default_icon": "Abekenman.svg",
    "default_popup": "popup.html"
  },
  "short_name": "Abekenman"
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display": "standalone",
  "start_url": "./index.html",
  "icons": [
    {
      "src": "Abekenman.png",
      "sizes": "48x48 72x72 128x128 256x256 2000x2000",
      "type": "image/png"
    },
    {
      "src": "Abekenman.svg",
      "type": "image/svg",
      "purpose": "maskable"
    },
    {
      "src": "Abekenman.svg",
      "sizes": "48x48 72x72 128x128 256x256 2000x2000",
      "type": "image/svg",
      "purpose": "maskable"
    }
  ],
  "version": "1.0",
  "version_name": "1.0"
}
