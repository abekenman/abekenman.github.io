{
  "author": "Abekenman",
  "background": {
    "scripts": ["background.js"],
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Abekenman"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "content_scripts": [
    {
      "matches": ["*://example.com/*"],
      "js": ["cs.js"],
      "run_at": "document_start",
      "css": ["cs.css"]
    }
  ],
  "content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
  "default_locale": "ja",
  "description": "【Abekenman】のPWA",
  "developer": {
    "name": "Abekenman",
    "url": "https://abekenman.github.io/about/abekenman/"
  },
  "name": "Abekenman",
  "short_name": "Abekenman",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display": "standalone",
  "start_url": "./index.html",
  "icons": [
    {
      "src": "Abekenman.png",
      "sizes": "48x48 72x72 128x128 256x256 2000x2000",
      "type": "image/png"
    },
    {
      "src": "Abekenman.svg",
      "type": "image/svg",
      "purpose": "maskable"
    },
    {
      "src": "Abekenman.svg",
      "sizes": "48x48 72x72 128x128 256x256 2000x2000",
      "type": "image/svg",
      "purpose": "maskable"
    }
  ]
}
