{
  "author": "Abekenman",
  "background": {
    "scripts": ["background.js"],
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Abekenman",
    "default_icon": "Abekenman.svg",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
